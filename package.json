    "@babel/preset-typescript": "7.12.1",
    "@types/lodash": "4.14.162",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@umijs/babel-preset-umi": "3.2.21",
    "@umijs/fabric": "2.2.2",
    "@umijs/preset-react": "1.6.12",
    "@umijs/utils": "3.2.23",
    "antd": "4.7.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.1",
    "chalk": "4.1.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.0",
    "eslint": "7.11.0",
    "father-build": "1.18.5",
