    "lerna": "3.22.1",
    "less-loader": "7.0.2",
    "lint-staged": "10.4.1",
    "lodash": "4.17.20",
    "mini-css-extract-plugin": "1.0.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.31",
